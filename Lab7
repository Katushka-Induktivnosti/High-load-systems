Task 8

> db.orders.mapReduce(function(){for (i in this.order_items){emit(this.order_items[i], this.customer.name+' '+this.customer.surname);}}, function(key, values){let customers = []; for (i in values){if (!customers.includes(values[i])){customers.push(values[i]);}}return customers.ToString;},{out:'task8'});
{
	"result" : "task8",
	"timeMillis" : 64,
	"counts" : {
		"input" : 4,
		"emit" : 8,
		"reduce" : 2,
		"output" : 6
	},
	"ok" : 1
}
> db.task8.find()
{ "_id" : "Fit", "value" : "Cathrine Sidorenko" }
{ "_id" : "Galaxy Fold", "value" : "Yekatierina Churakova" }
{ "_id" : "MotionFlow", "value" : "Kate Soldatova" }
{ "_id" : "Redmi Note", "value" : "Kate Soldatova" }
{ "_id" : "ThinQ", "value" : null }
{ "_id" : "iPhone12", "value" : null }

Task 9

> db.orders.mapReduce(function(){for (i in this.order_items){let customer = this.customer.name+' '+this.customer.surname; let item_user = {customer, item: this.order_items[i]}; emit(this.order_items[i]+' '+customer, item_user);}}, function(key, values){return values;},{out:'task91'});
{
	"result" : "task91",
	"timeMillis" : 73,
	"counts" : {
		"input" : 4,
		"emit" : 8,
		"reduce" : 0,
		"output" : 8
	},
	"ok" : 1
}
> db.task91.mapReduce(function(){if(this.value.length > 1){emit(this.value[0].item, this.value[0]);}}, function(key, values) {let customers = []; for (i in values){customers.push(values[i].customer);} return customers;},{out:'task9'});
{
	"result" : "task9",
	"timeMillis" : 54,
	"counts" : {
		"input" : 8,
