Task 1

> db.test.mapReduce(function(){return emit(this.producer, this.model)}, function(key, values){return values.length;},{out: 'task1'});
{
	"result" : "task1",
	"timeMillis" : 71,
	"counts" : {
		"input" : 9,
		"emit" : 9,
		"reduce" : 2,
		"output" : 6
	},
	"ok" : 1
}
> db.task1.find();
{ "_id" : "Apple", "value" : 2 }
{ "_id" : "Huawei", "value" : "Fit" }
{ "_id" : "LG", "value" : "ThinQ" }
{ "_id" : "Samsung", "value" : 3 }
{ "_id" : "Sony", "value" : "MotionFlow" }
{ "_id" : "Xiaomi", "value" : "Redmi Note" }

